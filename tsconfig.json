{
  "compilerOptions": {
    "typeRoots": [
      "node_modules/@types",
      "./app/@types"
    ],
    "types": [
      "express"
    ],
    "target": "es2017",
    "lib": [
      "ES6"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
  },
  "include": ["src"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
